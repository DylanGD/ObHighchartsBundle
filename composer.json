{
    "name": "ob/highcharts-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2/Symfony3 Bundle that ease the use of highcharts to display rich graphs, charts and maps in your app. This is a fork from the official release with the maps support added.",
    "keywords": ["symfony2", "symfony3", "charting", "charts", "highcharts", "chart", "graph", "graphs", "ob", "marcaube", "maps", "map", "dylangd"],
    "homepage": "https://github.com/DylanGD/ObHighchartsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dylan GD"
        }
    ],

    "require": {
        "php": ">=5.3",
        "symfony/symfony": "~2.3|~3.0|~4.0",
        "zendframework/zend-json": "~2.3|~3.0"
    },

    "autoload": {
        "psr-4": { "Ob\\HighchartsBundle\\": "" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "map-support"
        }
    }
}
